{
    "name": "clarity-seed",
    "version": "1.10.1",
    "description": "Angular-CLI starter for a Clarity project",
    "angular-cli": {},
    "scripts": {
        "start": "ng serve --host 0.0.0.0 --proxy-config local.proxy.conf.json",
        "start_prod": "ng serve --prod --host 0.0.0.0 --proxy-config local.proxy.conf.json",
        "start_container": "ng serve --prod --host 0.0.0.0 --proxy-config container.proxy.conf.json",
        "start_ssl": "ng serve --prod --host 0.0.0.0 --proxy-config local.proxy.conf.json --ssl true --ssl-cert ssl/service.crt --ssl-key ssl/server.key",
        "start_alt_ssl": "ng serve --prod --host 0.0.0.0 --port 8080 --proxy-config local.proxy.conf.json --ssl true --ssl-cert ./ssl/service.crt --ssl-key ./ssl/server.key",
        "start_ssl_container": "ng serve --prod --host 0.0.0.0 --proxy-config container.proxy.conf.json --ssl true --ssl-cert ssl/service.crt --ssl-key ssl/server.key",
        "deploy": "./deploy.sh",
        "lint": "tslint \"src/**/*.ts\"",
        "test": "ng test --single-run",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor protractor.config.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.2.3",
        "@angular/cli": "^8.3.0",
        "@angular/common": "8.2.3",
        "@angular/compiler": "8.2.3",
        "@angular/compiler-cli": "8.2.3",
        "@angular/core": "^8.2.3",
        "@angular/forms": "^8.2.3",
        "@angular/platform-browser": "8.2.3",
        "@angular/platform-browser-dynamic": "8.2.3",
        "@angular/router": "8.2.3",
        "@clr/angular": "^2.1.2",
        "@clr/icons": "^2.1.2",
        "@clr/ui": "2.1.2",
        "@types/core-js": "2.5.2",
        "@types/node": "^12.7.2",
        "@webcomponents/custom-elements": "1.2.4",
        "angular2-uuid": "^1.1.1",
        "bootstrap": "^4.3.1",
        "codelyzer": "^5.1.0",
        "enhanced-resolve": "4.1.0",
        "jquery": "3.4.1",
        "mutationobserver-shim": "~0.3.3",
        "ngx-cookie-service": "^2.2.0",
        "node-sass": "^4.12.0",
        "popper.js": "^1.15.0",
        "protractor": "^5.4.2",
        "rxjs": "^6.5.2",
        "typescript": ">=3.5.3",
        "web-animations-js": "~2.3.2",
        "webdriver-manager": "12.1.6",
        "yarn": "1.17.3",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.0",
        "@types/jasmine": "^3.4.0",
        "@types/jasminewd2": "~2.0.6",
        "jasmine-core": "^3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.2.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~2.0.0",
        "karma-jasmine": "~2.0.1",
        "karma-mocha-reporter": "~2.2.5",
        "npm-check": "^5.9.0",
        "nyc": "14.1.1",
        "ts-node": "8.3.0",
        "tslint": "^5.19.0"
    }
}
